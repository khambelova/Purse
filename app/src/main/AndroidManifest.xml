<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.habik.diplomapurse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Income.IncomeActivity"
            android:label="@string/income">
        </activity>

        <activity android:name=".Outcome.OutcomeActivity"
            android:label="@string/outcome">
        </activity>

        <activity android:name=".AllIncome.AllIncomeActivity"
            android:label="@string/all_income">
        </activity>

        <activity android:name=".AllOutcome.AllOutcomeActivity"
            android:label="@string/all_outcome">
        </activity>

        <activity android:name=".Balance.BalanceActivity"
            android:label="@string/balance">
        </activity>

        <activity android:name=".Income.CategoryIncomeActivity"
            android:label="@string/category">
        </activity>

        <activity android:name=".Outcome.CategoryOutcomeActivity"
            android:label="@string/category">
        </activity>

        <activity android:name=".CalendarActivity"
            android:label="@string/date">
        </activity>

        <activity android:name=".AllIncome.DiagramAllIncomeActivity"
            android:label="@string/diagramDate">
        </activity>

        <activity android:name=".AllOutcome.DiagramAllOutcomeActivity"
            android:label="@string/date">
        </activity>
            <activity android:name=".AllOutcome.AllOutcomeInCategoryActivity"
                android:label="@string/date">
            </activity>

            <activity android:name=".AllIncome.AllIncomeInCategoryActivity"
                android:label="@string/date">
            </activity>

    </application>

</manifest>